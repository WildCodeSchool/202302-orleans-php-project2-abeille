{% extends 'layout.html.twig' %}
{% block title %}
	{{parent()}}
	Admin|Faq
{% endblock %}
{% block stylesheet %}
	<link rel="stylesheet" href="/assets/css/adminfaq.css">
{% endblock %}
{% block navbar %}{% endblock %}
{% block content %}
	<main class="container">
		<h1>Administration Évènements</h1>
		<table>
			<thead>
				<tr>
					<th>Nom</th>
					<th>Description</th>
					<th>Location</th>
					<th>Date</th>
				</tr>
			</thead>
			<tbody>
				{% for event in events %}
					<tr>
						<td>{{ event.name }}</td>
						<td>{{ event.description }}</td>
						<td>{{event.location}}</td>
						<td>{{event.date}}</td>
					</tr>
				{% endfor %}		
			</tbody>
		</table>
	</main>

{% endblock %}
{% block footer %}{% endblock %}
